{
    "listener": [
        {
            "tcp": {
                "address": "0.0.0.0:{{vault_port}}",
                "tls_disable": 1
            }
        }
    ],
    "api_addr": "http://{{vault_address}}:{{vault_port}}",
    "storage": {
        "file": {
            "path": "{{vault_data_dir}}"
        }
    },
    "max_lease_ttl": "10h",
